{
  "version": 2,
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://backend-y4oa.onrender.com/api/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=63072000; includeSubDomains; preload" }
      ]
    }
  ]
}
